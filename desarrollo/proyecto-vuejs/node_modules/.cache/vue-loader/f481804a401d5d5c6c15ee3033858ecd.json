\r\n            <div class=\"form-group\">\r\n              <br>\r\n                <label for=\"edificio\">Edificio</label>\r\n                <select v-model=\"departamento.edificio\">\r\n                            <option v-for=\"e in edificiosList\" :key=\"e.url\" :value=\"e.url\">{{ e.nombre }} {{ e.tipo }}</option>\r\n                        </select>\r\n            </div>\r\n            <br>\r\n            <button type=\"submit\" class=\"btn btn-primary\">Crear</button>\r\n        </form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            departamento: {\r\n                nombre_propietario: '',\r\n                costo: '',\r\n                num_cuartos: '',\r\n                edificio: '',\r\n            },\r\n            edificiosList: [],\r\n            submitted: false\r\n        }\r\n    },\r\n    mounted() {\r\n        this.getEdificiosList()\r\n    },\r\n    methods: {\r\n      getEdificiosList() {\r\n            axios\r\n            .get('http://127.0.0.1:8090/api/edificios/')\r\n            .then(response => {\r\n                this.edificiosList = response.data.results\r\n            })\r\n            .catch(error => {\r\n                console.log(error)\r\n            })\r\n\r\n        },\r\n        create: function (e) {\r\n            this.$validator.validate().then(result => {\r\n                this.submitted = true;\r\n                if (!result) {\r\n                    return;\r\n                }\r\n                axios.post('http://127.0.0.1:8090/api/departamentos/',\r\n                        this.departamento\r\n                    )\r\n                    .then(response => {\r\n                        this.$router.push('/index');\r\n                    })\r\n            });\r\n        }\r\n    },\r\n}\r\n</script>\r\n"]}]}